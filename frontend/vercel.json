{
  "rewrites": [
     {
      "source": "/api/:path*",
      "destination": "https://webform-nine.vercel.app/api/:path*"
    },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install"
}